    "def find_min_index(arr):\n",
    "    minimum=float('inf')\n",
    "    for i,j in enumerate(arr):\n",
    "        if j<minimum:\n",
    "            return i\n",
    "    return None\n",
    "print(find_min_index([1,4,5,3,2,33,4,1,0]))"
   ]
